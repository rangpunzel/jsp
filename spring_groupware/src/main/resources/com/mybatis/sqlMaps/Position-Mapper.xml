<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Position-Mapper">
	
	<select id="selectPositionList" resultType='position'>
		select *
		from position
		where state=1
	</select>
</mapper>










